<markers>
<% @overviews.each_with_index do |post, index| %>
<marker nr="m<%= index %>" postid="<%= post.id %>" name="<%= post.user.name rescue nil%> <%= post.user.last_name rescue nil%>" address="<%= post.address %>" lat="<%= post.latitude %>" lng="<%= post.longitude %>" category="bar"></marker>
<% end %>
</markers>

