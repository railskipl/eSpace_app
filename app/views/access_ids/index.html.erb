
<h4>Manage College ID</h4>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Domain</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_ids.each do |access_id| %>
      <tr>
        <td><%= access_id.email %></td>
        <td><%= link_to 'Show', access_id %></td>
        <td><%= link_to 'Edit', edit_access_id_path(access_id) %></td>
        <td><%= link_to 'Destroy', access_id, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New', new_access_id_path %>
